let a=11;
let b=a+1;

let add=function(x,y){
	return x+y;
};

println(a,'*',b,'=',a*b);

println(a,'+',b,'=',add(a,b));

let gcd = function(x, y) {
	return x == y ? x : x < y ? gcd(x, y - x) : gcd(x - y, x);
};

var fib = function(n) {
	return n == 0 ? 0 : n == 1 ? 1 : fib(n - 1) + fib(n - 2);
};

var fact = function(n) {
	return n == 0 || n == 1 ? 1 : n * fact(n - 1);
};

var odd = function(n) {
	return n == 0 ? false : even(n - 1);
};

var even = function(n) {
	return n == 0 ? true : odd(n - 1);
};

println(11, ' is ', (even(11) ? '' : 'not '), 'even.');
println(11, ' is ', (odd(11) ? '' : 'not '), 'odd.');

println('The GCD of 81 and 27 is ' + gcd(81, 27));
println('The GCD of 81 and 18 is ' + gcd(81, 18));
println('The GCD of 99 and 16 is ' + gcd(99, 16));
println('');
println('fib(20)=' + fib(20));
println('');
println('fact(12)=' + fact(12));
println('');

for (var i=0;i<10;++i){
	if (odd(i+1))
		println('odd line ',i+1);
	else
		println('even line ',i+1);
}

