var testSuite={
	description: 'System Library',
	tests: [
		{
        	description: 'System.command',
        	test: function(){
				var filename='testdir.txt';
				File.delete(filename);
        		var result=
        			!File.exists(filename) &&
        			System.command('ls>'+filename)==0 &&
        			File.exists(filename);
        		File.delete('testdir.txt');
        		return result;
        	}
        },{
        	description: 'array push',
        	test: function(){
        		let a=[1,2,'3'];
        		Array.push(a,'4');
        		Array.push(a,5);
        		return a==[1,2,'3','4',5];
        	}
        },{
        	description: 'array pop',
        	test: function(){
        		let arr=[1,2,'3','4',5];
        		let a=Array.pop(arr);
        		let b=Array.pop(arr);
        		return arr==[1,2,'3'] && a==5 && b=='4';
        	}
        },{
        	description: 'array length',
        	test: function(){
        		let arr1=[1,2,'3','4',5];
        		let arr2=[];
        		return Array.length(arr1)==5 &&
        		       Array.length(arr2)==0 &&
        		       Array.length([1,2,'3','4',5])==5;
        	}
        },{
        	description: 'integer parse',
        	test: function(){
        		return Integer.parse('15')==15 &&
        			   Integer.parse('15.4')==Void &&
        		       Integer.parse('1.5a')==Void;
        	}
        },{
        	description: 'float parse',
        	test: function(){
        		return Float.parse('1.5')==1.5 &&
        		       Float.parse('1.5a')==Void;
        	}
        },{
        	description: 'float round',
        	test: function(){
        		return Float.round(12.34)==12 &&
        		       Float.round(12.64)==13;
        	}
        },
	]
};