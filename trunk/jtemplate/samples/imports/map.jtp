/***
Extensions to the Map map
***/

var Map.printMap=function(m){
    var keys=Map.keys(m);
    for (var i=0;i<Array.length(keys);i=i+1){
        println(keys[i],': ',m[keys[i]]);
    }
};

var Map.fold=function(f,m){
    var keys=Map.keys(m);
    for (var i=0;i<Array.length(keys);i=i+1){
        f(keys[i], m[keys[i]]);
    }
};